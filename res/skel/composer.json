{
    "name": "%vendor%/%package%",
    "description": "%Package% by %Vendor%",
    "keywords": ["keywords", "go", "here"],
    "homepage": "%github-url%",
    "license": "MIT",
    "authors": [
        {
            "name": "%name%",
            "email": "%email%"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "require-dev": {
        "icecave/testing": "~2"
    },
    "autoload": {
        "psr-0": {
            "%Vendor%\\%Package%": "%source%",
            "%Vendor%\\%Package%\\TypeCheck": "%source%-typhoon",
        }
    },
    "extra": {
        "typhoon": {
            "output-path": "%source%-typhoon",
            "validator-namespace": "%Vendor%\\%Package%\\TypeCheck",
            "use-native-callable": false
        }
    }
}
